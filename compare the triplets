



#include <bits/stdc++.h>
#include <iostream>

using namespace std;

int main(){
    int a=0,b=0,input1[4],input2[4];
    for(int i=0;i<3;i++){
        cin>>input1[i];
    }
    for(int j=0;j<3;j++){
        cin>>input2[j];
    }
    for(int k=0;k<3;k++){
        if(input1[k]>input2[k]){
            a=a+1;
        }else if (input1[k]<input2[k]) {
            b=b+1;
        }
    }
    cout<<a<<" "<<b<<endl;
}
